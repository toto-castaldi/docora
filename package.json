    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "@octokit/rest": "^22.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.66.4",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
